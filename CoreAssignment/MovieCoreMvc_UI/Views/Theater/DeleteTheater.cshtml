@model CoreEntity.Theater


<div> 
    <a asp-controller="Theater" asp-action="TheaterEntry">Back to Theater Entry</a>
    <center><h1>Welcome to Theater Delete Section</h1></center>
<hr />
</div>

<form method="post" enctype="multipart/form-data">
    <h2>Theater Entry</h2>
    <div class=" form-group">
        <label>Theater ID</label>
        <input type="text" asp-for="Id" class="form-control" placeholder="enter theater id" required />
    </div>
   <input type="submit" value="Save" class="btn btn-outline-success" />
    <input type="button" value="Reset" class="btn btn-outline-secondary" />
    <hr />
    @if (ViewBag.status == "Ok")
    {
        <div class="alert alert-success">
            @ViewBag.message
        </div>
    }
    else if (ViewBag.status == "Error")
    {
        <div class="alert alert-danger">
            @ViewBag.message
        </div>
    }
</form>
